{
  "name": "@soulofmischief/vector.js",
  "description": "A small 3D Vector class written in ES6+Flow",
  "version": "1.1.2",
  "author": {
    "name": "soulofmischief",
    "email": "soulofmischief@protonmail.com",
    "url": "https://github.com/soulofmischief"
  },
  "homepage": "https://github.com/soulofmischief/Vector.js",
  "license": "MIT",
  "bugs": {},
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "repository": {},
  "scripts": {
    "build": "npm run clean; for script in {commonjs,es,umd,umd:min}; do npm run build:$script; done",
    "build:commonjs": "BABEL_ENV=commonjs npx babel src --out-dir lib",
    "build:es": "BABEL_ENV=es npx babel src --out-dir es",
    "build:umd": "BABEL_ENV=es NODE_ENV=development npx rollup src/index.js --config --sourcemap --file dist/vector.js",
    "build:umd:min": "BABEL_ENV=es NODE_ENV=production npx rollup src/index.js --config --file dist/vector.min.js",
    "clean": "npx rimraf es dist lib",
    "lint:flow": "npx flow status",
    "lint:flow:stop": "npx flow stop",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.89.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1"
  },
  "dependencies": {}
}
